{
    "name": "adminlte",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "start": "ng serve -o",
        "build": "ng build --configuration production",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "update": "ng update --force @angular/cli @angular/core",
        "store-version": "echo '{\"version\" : '$(cat package.json | jq .version )'}' > public/version.json && git add public/version.json && git commit -m \"version upgrade \"",
        "release:patch": "standard-version --release-as patch",
        "release:minor": "standard-version --release-as minor",
        "release:major": "standard-version --release-as major"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
        }
    },
    "dependencies": {
        "@angular/animations": "18.0.2",
        "@angular/common": "18.0.2",
        "@angular/compiler": "18.0.2",
        "@angular/core": "18.0.2",
        "@angular/forms": "18.0.2",
        "@angular/localize": "^18.0.2",
        "@angular/platform-browser": "18.0.2",
        "@angular/platform-browser-dynamic": "18.0.2",
        "@angular/router": "18.0.2",
        "@ng-bootstrap/ng-bootstrap": "^16.0.0",
        "@ng-idle/core": "^14.0.0",
        "@ng-select/ng-select": "^13.4.1",
        "@ngrx/store": "17.1.0",
        "@popperjs/core": "^2.11.8",
        "@profabric/angular-components": "^0.2.3",
        "angular-datatables": "^17.1.0",
        "bootstrap": "5.3.2",
        "core-js": "3.35.1",
        "datatables.net": "^2.0.8",
        "datatables.net-bs5": "^2.0.8",
        "datatables.net-dt": "^2.0.8",
        "jquery": "^3.7.1",
        "lit": "3.1.1",
        "luxon": "^3.4.4",
        "ngx-spinner": "^17.0.0",
        "ngx-toastr": "^18.0.0",
        "process": "0.11.10",
        "rxjs": "7.8.1",
        "summernote": "./summernote",
        "sweetalert2": "^11.11.1",
        "tslib": "2.6.2",
        "uuid": "9.0.1",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "18.0.3",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "18.0.3",
        "@angular/compiler-cli": "18.0.2",
        "@angular/language-service": "18.0.2",
        "@types/datatables.net": "^1.12.0",
        "@types/jasmine": "~5.1.4",
        "@types/jasminewd2": "2.0.13",
        "@types/jquery": "^3.5.30",
        "@types/node": "20.11.5",
        "@typescript-eslint/eslint-plugin": "6.19.1",
        "@typescript-eslint/parser": "6.19.1",
        "admin-lte": "^3.2.0",
        "eslint": "8.56.0",
        "fs-web": "1.0.1",
        "husky": "8.0.3",
        "jasmine-core": "~5.1.1",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "6.4.2",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "lint-staged": "15.2.0",
        "os-browserify": "0.3.0",
        "path-browserify": "1.0.1",
        "prettier": "3.2.4",
        "pretty-quick": "4.0.0",
        "protractor": "7.0.0",
        "standard-version": "9.5.0",
        "ts-node": "10.9.2",
        "tslint-config-prettier": "1.18.0",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "5.4.5"
    }
}
